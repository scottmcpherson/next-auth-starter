{
  "name": "next-auth-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:seed": "dotenv -e .env.local -- node -r esbuild-register db/seed.ts",
    "db:migrations:generate": "dotenv -e .env.local -- drizzle-kit generate:pg --config=./db/drizzle.config.ts",
    "db:migrations:push": "dotenv -e .env.local -- node -r esbuild-register db/migrate.ts --config=./db/drizzle.config.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@radix-ui/react-avatar": "^1.0.4",
    "@vercel/postgres": "^0.5.1",
    "clsx": "^2.0.0",
    "cookiejs": "^2.1.2",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.19.13",
    "drizzle-orm": "^0.28.6",
    "firebase": "^10.5.2",
    "firebase-admin": "^11.11.0",
    "next": "14.0.1",
    "next-themes": "^0.2.1",
    "nookies": "^2.5.2",
    "openai-edge": "^1.2.2",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
